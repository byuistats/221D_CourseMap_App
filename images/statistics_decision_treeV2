digraph {
	"Design the Study"
	B [label="What is your Response 
 variable?" fillcolor=lightgray shape=diamond style=filled]
	B -> Q [label=Quantitative]
	B -> C [label=Categorical]
	Q [label="What is your Explanatory 
 Variable?" fillcolor=forestgreen shape=diamond style=filled]
	Q -> R [label=None]
	R [label="Mean, Median, Mode, 
 Standard Deviation, Percentiles
Graphs: Histogram" fillcolor=mediumseagreen shape=rectangle style=filled]
	R -> S [label="Inferential Statistics"]
	S [label="One-sample t-test: 
 t = (x̄ - μ) / (s / √n) 
 CI: x̄ ± t*s/√n" fillcolor=palegreen shape=rectangle style=filled]
	Q -> cat [label=Categorical]
	cat [label="Grouped Means, Medians, 
 Standard Deviations, Percentiles
Graphs: side-by-side Boxplot" fillcolor=mediumseagreen shape=rectangle style=filled]
	cat -> T [label="Inferential Statistics"]
	T [label="How Many 
 Groups?" fillcolor=mediumseagreen shape=diamond style=filled]
	T -> dep [label="2 Dependent Groups"]
	dep [label="Matched Pairs t-test: 
 t = (d̄ - μ₀) / (s_d / √n) 
 CI: d̄ ± t*s_d/√n" fillcolor=palegreen shape=rectangle style=filled]
	T -> indep [label="2 Independent Groups"]
	indep [label="2-sample t-test: 
 t = (x̄₁ - x̄₂) / √((s₁²/n₁) + (s₂²/n₂)) 
 CI:  (x̄₁ - x̄₂) ± t*√((s₁²/n₁) + (s₂²/n₂))" fillcolor=palegreen shape=rectangle style=filled]
	T -> mult [label=">2 Independent Groups"]
	mult [label=<<b>ANOVA:</b><br/>F = MST / MSE<br/>MST = SST / (k - 1)<br/>MSE = SSE / (n - k)> fillcolor=palegreen shape=rectangle style=filled]
	Q -> SLR_Desc [label=Quantitative]
	SLR_Desc [label="Scatter plot, 
 geom_smooth()" fillcolor=mediumseagreen shape=rectangle style=filled]
	SLR_Desc -> SLR_Infer [label="Inferential Statistics"]
	SLR_Infer [label="Regression Equation:
 ŷ = b₀ + b₁x
b₁ = r(sᵧ/sₓ)
Hypothesis Test for Slope:
 t = b₁ / SE(b₁)" fillcolor=palegreen shape=rectangle style=filled]
	C [label="What is your Explanatory 
 Variable?" fillcolor=royalblue shape=diamond style=filled]
	C -> P1_Desc [label=None]
	P1_Desc [label="Proportion (p̂)
Graphs: Bar chart, Pie chart" fillcolor=steelblue shape=rectangle style=filled]
	P1_Desc -> P1_Infer [label="Inferential Statistics"]
	P1_Infer [label="1-Sample z-test for proportions:
 z = (p̂ - p₀) / √(p₀(1-p₀)/n)
CI: p̂ ± z*√(p̂(1-p̂)/n)" fillcolor=lightblue shape=rectangle style=filled]
	C -> P2_Desc [label="2 Groups"]
	P2_Desc [label="Proportions (p̂₁, p̂₂)
Graphs: Clustered bar chart" fillcolor=steelblue shape=rectangle style=filled]
	P2_Desc -> P2_Infer [label="Inferential Statistics"]
	P2_Infer [label="2-Sample z-test for proportions:
 z = (p̂₁ - p̂₂) / √(p̂(1-p̂)(1/n₁ + 1/n₂))
CI: (p̂₁ - p̂₂) ± z*√(p̂(1-p̂)(1/n₁ + 1/n₂))" fillcolor=lightblue shape=rectangle style=filled]
	C -> Chi_Desc [label="2 Variables"]
	Chi_Desc [label="Two-way table
Graphs: Clustered bar chart" fillcolor=steelblue shape=rectangle style=filled]
	Chi_Desc -> Chi_Infer [label="Inferential Statistics"]
	Chi_Infer [label="Chi-Square Test:
 χ² = Σ((O - E)² / E)
Degrees of Freedom: (r - 1)(c - 1)" fillcolor=lightblue shape=rectangle style=filled]
}
